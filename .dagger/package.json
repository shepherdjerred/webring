{
  "name": "@webring/dagger",
  "type": "module",
  "private": true,
  "packageManager": "npm@10.9.2",
  "scripts": {
    "postinstall": "echo 'export * from \"./dist/src/index.js\";' > node_modules/@dagger.io/dagger/index.js 2>/dev/null || true",
    "build": "echo 'Dagger module ready'",
    "test": "echo 'No tests configured for Dagger module'",
    "type-check": "npm run tsc -- --noEmit",
    "lint": "npx eslint src",
    "format": "npx prettier --check src",
    "format:write": "npx prettier --write src",
    "typecheck": "npx tsc --noEmit"
  },
  "dependencies": {
    "@dagger.io/dagger": "^0.19.8",
    "@shepherdjerred/dagger-utils": "^0.1.0",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript-eslint": "^8.26.1"
  },
  "overrides": {
    "@dagger.io/dagger": "^0.19.8"
  }
}
